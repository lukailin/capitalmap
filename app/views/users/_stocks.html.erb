
  <div id="stocks">
    <% if current_user.stocks.exists? %>
      <% current_user.stocks.each do |e| %>
        <% if current_user.stocks.last != e %>
          <!-- Do nothing -->
        <% else %>
            <div class="charts"><img src="http://chart.finance.yahoo.com/z?s=<%= e.ticker %>&t=3m&q=c&l=on"></div>
        <% end %>
      <% end %>
    <% end %> 
  </div>